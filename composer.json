{
    "name": "wrm/events",
    "type": "typo3-cms-extension",
    "description": "Extension to manage Destination Data managed events",
    "authors": [
        {
            "name": "Dirk Koritnik",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Wrm\\Events\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wrm\\Events\\Tests\\": "Tests"
        }
    },
    "replace": {
        "events": "self.version",
        "typo3-ter/events": "self.version"
    }
}